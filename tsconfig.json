{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
    "declaration": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@react-admin/pro-codemirror": ["./packages/codemirror/src/index.tsx"],
      "@react-admin/pro-components": ["./packages/components/src/index.tsx"],
      "@react-admin/pro-form": ["./packages/form/src/index.tsx"],
      "@react-admin/input": ["./packages/input/src/index.tsx"],
      "@react-admin/scroll-select": ["./packages/scroll-select/src/index.tsx"],
      "@react-admin/select": ["./packages/select/src/index.tsx"],
      "@react-admin/spin": ["./packages/spin/src/index.tsx"],
      "@react-admin/table-page": ["./packages/table-page/src/index.tsx"],
      "@react-admin/upload": ["./packages/upload/src/index.tsx"],
      "@react-admin/pro-utils": ["./packages/utils/src/index.tsx"]
    },
    "ignoreDeprecations": "5.0",
    "module": "ES2022"
  },
  "include": [
    "./tests/setupTests.ts",
    "jest.config.ts",
    ".dumirc.ts",
    "typings.d.ts",
    "**/src/**/*",
    "**/docs/**/*",
    "scripts/**/*",
    "**/demos",
    ".eslintrc.js",
    "tests",
    "jest.config.ts",
    "**/fixtures",
    "./tests/no-duplicated.ts",
    "packages/descriptions/src/demos/.tsx"
  ]
}
