{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
    "declaration": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "@hs-react-admin/base-components": [
        "./packages/base-components/src/index.tsx"
      ],
      "@hs-react-admin/utils": [
        "./packages/utils/src/index.tsx"
      ]
    },
    "ignoreDeprecations": "5.0"
  },
  "include": [
    "./tests/setupTests.ts",
    "jest.config.ts",
    ".dumirc.ts",
    "typings.d.ts",
    "**/src/**/*",
    "**/docs/**/*",
    "scripts/**/*",
    "**/demos",
    ".eslintrc.js",
    "tests",
    "jest.config.ts",
    "**/fixtures",
    "./tests/no-duplicated.ts",
    "packages/descriptions/src/demos/.tsx"
  ]
}