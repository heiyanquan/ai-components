{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "hs-react-admin": [
        "packages/base-components"
      ],
      "hs-react-admin/*": [
        "packages/base-components/*",
        "*"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "packages/base-components/**/*"
  ]
}